{
  "name": "porfolio-monalisa",
  "version": "1.13.0",
  "description": "Monalisa's portfolio showcasing her experience and contacts in a visually appealing manner",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Algostract/portfolio-monalisa.git"
  },
  "type": "module",
  "private": true,
  "volta": {
    "node": "22.14.0"
  },
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0"
  },
  "packageManager": "bun@1.2.13",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:load": "k6 run tests/load.test.ts",
    "build": "nuxt build && cp -r static .output/static",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node --env-file=.env.prod .output/server/index.mjs",
    "clean": "nuxt cleanup",
    "docker:build": "docker build --build-arg TAG=dev -t portfolio-monalisa:dev .",
    "docker:start": "docker run --detach --name portfolio-monalisa-dev --mount type=bind,source=$(pwd)/static,target=/app/static,readonly --env-file .env.prod -p 4101:3000 portfolio-monalisa:dev",
    "tunnel": "untun tunnel --port 4101 http://localhost:4101"
  },
  "dependencies": {
    "@dicebear/collection": "^9.2.2",
    "@dicebear/core": "^9.2.2",
    "@notionhq/client": "^2.3.0",
    "@nuxt/eslint": "^1.3.1",
    "@nuxt/fonts": "^0.11.3",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "0.11.5",
    "@nuxt/test-utils": "^3.18.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "3.0.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@types/k6": "^1.0.2",
    "@unhead/vue": "^2.0.8",
    "@unovis/ts": "^1.5.1",
    "@unovis/vue": "^1.5.1",
    "@vite-pwa/nuxt": "^1.0.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/nuxt": "^13.1.0",
    "confbox": "^0.2.2",
    "date-fns": "^4.1.0",
    "eslint": "^9.26.0",
    "modern-screenshot": "^4.6.0",
    "notion-to-md": "^3.1.9",
    "nuxt": "^3.17.3",
    "nuxt-splide": "^1.1.1",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3",
    "untun": "^0.1.3"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "esbuild",
    "maplibre-gl",
    "sharp",
    "unrs-resolver"
  ]
}
