{
  "name": "porfolio-monalisa",
  "version": "1.16.0",
  "description": "Monalisa's portfolio showcasing her experience and contacts in a visually appealing manner",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Algostract/portfolio-monalisa.git"
  },
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0",
    "bun": "^1.2.9"
  },
  "packageManager": "bun@1.2.18",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:load": "k6 run tests/load.test.ts",
    "build": "nuxt build && cp -r static .output/static",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node --env-file=.env.prod .output/server/index.mjs",
    "clean": "nuxt cleanup",
    "docker:build": "docker build --build-arg TAG=dev -t portfolio-monalisa:dev .",
    "docker:start": "docker run --detach --name portfolio-monalisa-dev --mount type=bind,source=$(pwd)/static,target=/app/static,readonly --env-file .env.prod -p 4200:3000 portfolio-monalisa:dev",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:dev:mobile": "tauri android dev",
    "tauri:build:mobile": "tauri android build"
  },
  "dependencies": {
    "@dicebear/collection": "^9.2.3",
    "@dicebear/core": "^9.2.3",
    "@notionhq/client": "^3.1.3",
    "@nuxt/eslint": "^1.5.2",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "0.11.5",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "3.0.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@types/k6": "^1.1.1",
    "@types/web-push": "^3.6.4",
    "@unhead/vue": "^2.0.12",
    "@unovis/ts": "^1.5.2",
    "@unovis/vue": "^1.5.2",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "confbox": "^0.2.2",
    "date-fns": "^4.1.0",
    "eslint": "^9.31.0",
    "notion-to-md": "^3.1.9",
    "nuxt": "^3.17.6",
    "nuxt-splide": "^1.1.1",
    "vue": "latest",
    "vue-router": "latest",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tauri-apps/cli": "^2.6.2",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "playwright-core": "^1.54.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "esbuild",
    "maplibre-gl",
    "sharp",
    "unrs-resolver"
  ]
}
